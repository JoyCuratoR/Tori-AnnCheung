<script>
// Function to convert a date string to a Date object
    function parseDate(dateString) {
      var dateParts = dateString.split("-");
      return new Date(dateParts[0], dateParts[1] - 1, dateParts[2]);
    }

    // Sort the title cards by publish date
    function sortTitleCards() {
      var titleCardsContainer = document.getElementById("title-cards-container");
      var titleCards = Array.from(titleCardsContainer.getElementsByClassName("title-card"));

      // Sort the title cards based on the publish date
      titleCards.sort(function(a, b) {
        var aDate = parseDate(a.getElementsByClassName("publish-date")[0].textContent);
        var bDate = parseDate(b.getElementsByClassName("publish-date")[0].textContent);
        return bDate - aDate;
      });

      // Clear the title cards container
      titleCardsContainer.innerHTML = "";

      // Add the sorted title cards back to the container
      titleCards.forEach(function(card) {
        titleCardsContainer.appendChild(card);
      });
    }

    // Call the sortTitleCards function when the page finishes loading
    window.addEventListener("load", sortTitleCards);



  function filterByTag(tag) {
    // Get all the title cards
    const titleCards = document.getElementsByClassName('title-card');

    // Show or hide the title cards based on the selected tag
    for (let i = 0; i < titleCards.length; i++) {
      const titleCard = titleCards[i];
      const tags = titleCard.getElementsByClassName('tag');
      let showCard = false;

      // Check if the tag matches or if the selected tag is 'all'
      for (let j = 0; j < tags.length; j++) {
        if (tag === 'all' || tags[j].innerText.toLowerCase().includes('#' + tag.toLowerCase())) {
          showCard = true;
          break;
        }
      }

      // Set the display property based on the showCard flag
      titleCard.style.display = showCard ? 'flex' : 'none';
    }
  }
</script>

<!-- Search bar functionality -->
<script>
  function search() {
    const searchInput = document.getElementById('search-input');
    const searchText = searchInput.value.toLowerCase();
    
    // Get all the title cards
    const titleCards = document.getElementsByClassName('title-card');
    
    // Loop through each title card and check if it matches the search text
    for (let i = 0; i < titleCards.length; i++) {
      const titleCard = titleCards[i];
      const title = titleCard.getElementsByTagName('h2')[0].innerText.toLowerCase();
      
      // If the title contains the search text, display the title card, otherwise hide it
      if (title.includes(searchText)) {
        titleCard.style.display = 'flex'; // Set display to flex
      } else {
        titleCard.style.display = 'none'; // Hide the title card
      }
    }
  }
</script>

